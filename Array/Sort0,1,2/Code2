package com.array.revise;

public class DutchFlag {

	public static void main(String[] args) {
		int[] nums= {2,1,0,0,1,2,1,0};

		int low=0;int mid=0;int high=nums.length-1;

		while(mid<=high)
		{
			if(nums[mid]==0)
			{
				nums[mid]=nums[low];
				nums[low]=0;
				low++;
			}

			if(nums[mid]==2)
			{
				nums[mid]=nums[high];
				nums[high]=2;
				high--;
				mid--;
			}

		mid++;


		}

		
		for(int i=0;i<nums.length;i++)
			System.out.print(nums[i]+" ");
		

	}

}
